{% extends 'base.html.twig' %}

{% block title %}{% endblock %}
{% block body %}
{% for alert in app.flashes('alert') %}
    <div class="alert bg-danger" role="alert"><em class="fa fa-minus-circle mr-2"></em>
        {{ alert }}
    </div>
{% endfor %}
{{ form_start(form) }}
{{ form_widget(form) }}

<input type="submit" class="btn btn-default">
{{ form_end(form) }}
{% endblock %}